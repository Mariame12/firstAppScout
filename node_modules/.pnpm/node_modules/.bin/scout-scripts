#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mariamesasconte/Documents/Sepro/Scoot/contactscout/contactscout/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/mariamesasconte/Documents/Sepro/Scoot/contactscout/contactscout/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@eclipse-scout/cli/bin/scout-scripts.js" "$@"
else
  exec node  "$basedir/../@eclipse-scout/cli/bin/scout-scripts.js" "$@"
fi
